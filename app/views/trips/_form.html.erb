<%= form_for @trip do |f| %>
<%= f.label :date %>
<%= f.text_field :date %>

<%= f.label :rating %>
<%= f.number_field :rating %>

<%= f.label :cost %>
<%= f.number_field :cost %>

<%= f.label :driver_id %>
<%= f.number_field :driver_id %>

<%= f.label :passenger_id %>
<%= f.number_field :passenger_id %>

<%= f.submit %>
<% end %>
